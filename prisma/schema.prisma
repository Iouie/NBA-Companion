generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}

// npx prisma generate   adds to local
// npx prisma db push    add it to planetscale

model Category{
  id           String @id @default(uuid())
  name         String
}